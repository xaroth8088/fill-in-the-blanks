{
  "name": "Fill in the blanks",
  "description": "It's like MadLibs, except a) it's not affiliated with that word game, and b) it (theoretically) works on any webpage",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Make it more fun!"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}
